    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
    xmlns:toolkit="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone.Controls.Toolkit"
    xmlns:local="clr-namespace:Sudokudos"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    SupportedOrientations="Portrait" Orientation="Portrait"
    shell:SystemTray.IsVisible="True"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    OrientationChanged="PhoneApplicationPage_OrientationChanged"
    ManipulationStarted="PhoneApplicationPage_ManipulationStarted"
    ManipulationDelta="PhoneApplicationPage_ManipulationDelta"
    ManipulationCompleted="PhoneApplicationPage_ManipulationCompleted"
    Tap="PhoneApplicationPage_Tap"
    Loaded="PhoneApplicationPage_Loaded">
    
    <Canvas x:Name="PageCanvas" Tap="PageCanvas_Tap">
        
        <StackPanel x:Name="TitlePanel"
            Canvas.Top="0" Canvas.ZIndex="1" Tap="TitlePanel_Tap"
            HorizontalAlignment="Stretch" Width="480">

            <TextBlock x:Name="levelText" Text="Level 1" HorizontalAlignment="Center" FontFamily="{StaticResource PhoneFontFamilySemiLight}" FontSize="{StaticResource PhoneFontSizeExtraExtraLarge}" />
        </StackPanel>

        <Grid x:Name="LayoutRoot" Background="Transparent" Canvas.Top="125"
        Canvas.ZIndex="0" Tap="LayoutRoot_Tap">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <Grid.Resources>
                <System:Double x:Key="cellHeight">45<!--40--></System:Double>
                <System:Double x:Key="cellWidth">40<!--36--></System:Double>
                <System:Double x:Key="cellFontSize">36</System:Double>
                <FontWeight x:Key="cellFontWeight">Bold</FontWeight>
                <!--<Color x:Key="userEnteredColor"></Color>-->

                <System:Double p:Key="tappadHeight">40</System:Double>
                <System:Double p:Key="tappadWidth">90</System:Double>
                <System:Double p:Key="tappadOpacity">0</System:Double>

                <System:Double x:Key="sharedFrom">0</System:Double>
                <System:Double x:Key="sharedTo">100</System:Double>
                <System:Int32 x:Key="sharedOscillations">4</System:Int32>
                <System:Double x:Key="sharedSpringiness">15</System:Double>

                <System:Double x:Key="rightSliderInitialXOffset">75</System:Double>
                <System:Double x:Key="numPickerInitialYOffset">200</System:Double>
                <System:Double x:Key="pickerFontSize"><!--55-->62</System:Double>
                
                <SolidColorBrush x:Key="y0Brush" Color="#CC880040" />
                <SolidColorBrush x:Key="y1Brush" Color="#CC004088" />
                <SolidColorBrush x:Key="y2Brush" Color="#CC74E800" />

                <ImageBrush x:Key="nebula" ImageSource="/Resources/Images/nebula.png" />
                <ImageBrush x:Key="red"    ImageSource="/Resources/Images/red.png" />
                <ImageBrush x:Key="starry" ImageSource="/Resources/Images/starry.png" />
                <ImageBrush x:Key="eye"    ImageSource="/Resources/Images/eye.jpg" />
                <ImageBrush x:Key="blue"   ImageSource="/Resources/Images/blue.png" />

                <Storyboard x:Name="PickerInOut" FillBehavior="HoldEnd" Completed="PickerInOut_Completed">
                    <DoubleAnimation x:Name="anim_numPicker"
                                Storyboard.TargetName="numPicker_tran"
                                Storyboard.TargetProperty="Y"
                                From="{StaticResource numPickerInitialYOffset}"
                                To="0" Duration="0:0:0.6">
                        <DoubleAnimation.EasingFunction>
                            <BounceEase EasingMode="EaseOut" Bounciness="3"/>
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>

                <Storyboard x:Name="SliderInOut" FillBehavior="HoldEnd" Completed="SliderInOut_Completed">
                    <DoubleAnimation x:Name="anim_planeSelectSliderRight"
                                Storyboard.TargetName="planeSelectSliderRight_tran"
                                Storyboard.TargetProperty="X"
                                From="{StaticResource rightSliderInitialXOffset}"
                                To="0" Duration="0:0:1.0">
                        <DoubleAnimation.EasingFunction>
                            <BounceEase EasingMode="EaseOut" />
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>

                <Storyboard x:Name="Twistidoo" FillBehavior="HoldEnd"  Completed="storyBoard_Completed">

                    <DoubleAnimation x:Name="anim_0_0_0_X"
                            Storyboard.TargetName="cell_0_0_0_tran" 
                            Storyboard.TargetProperty="X" 
                            From="{StaticResource sharedFrom}" To="{StaticResource sharedTo}" 
                            Duration="0:0:1">
